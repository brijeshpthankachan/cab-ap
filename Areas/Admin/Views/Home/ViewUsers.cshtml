@model IEnumerable<ApplicationUser>


<div class="pt-5 mt-5 container">
    <div class="w-100 p-2">
        <table id="example" class="table mt-4" style="width: 100%">
            <thead>
            <tr>
                <th>Name</th>
                <th>Phone Number</th>
                <th>Email</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            @try
            {
                @foreach (var i in @Model)
                {
                    <tr>
                        <td>@i.FirstName @i.LastName </td>
                        <td>@i.PhoneNumber</td>
                        <td>@i.Email</td>
                        <td>
                            <a class="btn btn-primary">Delete</a>
                        </td>
                    </tr>
                }
            }
            catch (Exception e)
            {
                Console.WriteLine(e);
            }
            </tbody>
        </table>
    </div>
</div>